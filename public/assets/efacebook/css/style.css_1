/**/
.character {
}
.arrow_left {
    background:url(../images/arrow_left.png) no-repeat;
    -webkit-background-size: auto 16px;
    -moz-background-size: auto 16px;
    background-size:auto 16px;
    width:10px;
    height:16px;
    position:absolute;
    top:60px;
    left:13px;
    opacity:0.5;
    z-index:2;
    animation: arrowMoveLeft 0.5s infinite;
    -webkit-animation: arrowMoveLeft 0.5s infinite;
}

@keyframes arrowMoveLeft
{
    0%   {left:0px; opacity:0.1}
50%  {left:5px; opacity:1}
100% {left:0px; opacity:0.1}
}
@-webkit-keyframes arrowMoveLeft
{
    0%   {left:0px; opacity:0.1}
50%  {left:5px; opacity:1}
100% {left:0px; opacity:0.1}
}
.arrow_right {
    background:url(../images/arrow_right.png) no-repeat;
    -webkit-background-size: auto 16px;
    -moz-background-size: auto 16px;
    background-size:auto 16px;
    width:10px;
    height:16px;
    position:absolute;
    top:60px;
    right:13px;
    opacity:0.5;
    z-index:2;
    animation: arrowMoveRight 0.5s infinite;
    -webkit-animation: arrowMoveRight 0.5s infinite;
}

@keyframes arrowMoveRight
{
    0%   {right:0px; opacity:0.1}
50%  {right:5px; opacity:1}
100% {right:0px; opacity:0.1}
}
@-webkit-keyframes arrowMoveRight
{
    0%   {right:0px; opacity:0.1}
50%  {right:5px; opacity:1}
100% {right:0px; opacity:0.1}
}
