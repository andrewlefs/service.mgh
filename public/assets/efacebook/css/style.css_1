.spe_gradient {
    background: -moz-linear-gradient(left,  rgba(132,105,82,0) 0%, rgba(132,105,82,1) 50%, rgba(132,105,82,0) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(132,105,82,0)), color-stop(50%,rgba(132,105,82,1)), color-stop(100%,rgba(132,105,82,0))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left,  rgba(132,105,82,0) 0%,rgba(132,105,82,1) 50%,rgba(132,105,82,0) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left,  rgba(132,105,82,0) 0%,rgba(132,105,82,1) 50%,rgba(132,105,82,0) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left,  rgba(132,105,82,0) 0%,rgba(132,105,82,1) 50%,rgba(132,105,82,0) 100%); /* IE10+ */
    background: linear-gradient(to right,  rgba(132,105,82,0) 0%,rgba(132,105,82,1) 50%,rgba(132,105,82,0) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00846952', endColorstr='#00846952',GradientType=1 ); /* IE6-9 */

    height:2px;
    width:100%;
    position:relative;
}
